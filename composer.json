{
    "name": "fighttheice/code",
    "description": "PHP code generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FightTheIce\\Code\\": "src/"
        }
    },
    "authors": [
        {
            "name": "William Knauss",
            "email": "william.knauss@kingdom.com"
        }
    ],
    "require": {
        "laminas/laminas-code": "^4.8",
        "nette/php-generator": "^4.0",
        "nikic/php-parser": "^4.15",
        "dflydev/dot-access-data": "^3.0",
        "laminas/laminas-stdlib": "^3.16"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.7",
        "vimeo/psalm": "^5.7",
        "phpstan/phpstan": "^1.9"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
